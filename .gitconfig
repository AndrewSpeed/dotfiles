[user]
    name = Andrew Speed
    email = aspeed@cloudsmith.io
[alias]
    st = status
    co = checkout
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[branch]
    sort=-committerdate
[color]
    ui = true
[core]
    editor = vim
    autocrlf = input
[help]
    autocorrect = 1
[push]
    default = simple
[merge]
    tool = vimdiff
[mergetool]
    prompt = false
